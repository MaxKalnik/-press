  .full-table
    display: table
    width: 100%
    overflow scroll
  .mobile-only-show
    display: none
  .mobile-only-close
    display: block



@media (max-width: 759px)
  .content
    margin-left: 10px
    margin-right: 10px

  .top-logo
    display: none
    
  .mob-logo
    display: block
    width: 35px
    height: 38px

  .breadcrumbs__wrap
    white-space: nowrap

    max-width: 100%
    box-sizing border-box

  .breadcrumbs__item:last-child
    overflow: hidden
    text-overflow: ellipsis

  .article-title
    font-size: 22px
    line-height: 30px
    font-weight: 600

  .article-text__item
    min-width: 265px
    width: auto
    font-size: 16px
    line-height: 24px
    font-weight: 300
    margin-bottom: 12px
    
  .article-author
    font-size: 12px
    line-height: 16px
    font-weight: 300

  .main-pic
    height: 345px
    padding: 16px 12px 16px 12px
    background-position: 27% 10%
    background-image url("images/main_pic_320@2x.png")
  
  .main-pic__mask
    height: 170px

  .text-box
    padding: 32px 12px 32px 12px
    &--upper
      justify-content: center
      .upper-pic
        margin-bottom: 32px
      .under-title
        width: 100%
      
      .under-title
        font-size: 16px
        line-height: 28px
        font-weight: 200
        
      .main-text
        font-size: 14px
        line-height: 24px
        font-weight: 300

  .calendar
    &__img
      img
        width: 276px
      
  .ontime-rank
    box-sizing: border-box
    .table
      width: 100%
      &__head
        text-align: left
        font-size: 12px
        line-height: 20px
        top: 15px
        left: 100px
    
      table
        tbody
          display: flex
          flex-direction: column
          flex-wrap: wrap
          tr
            width: 100%
            display: flex
            flex-direction: row
            td
              padding-top: 14px
              &:first-child
                width: 10%
                padding-left: 10px

              &:nth-child(2)
                width: 80%
                padding-left: 8px
                padding-top: 16px

              &:nth-child(3)
                width: auto
                padding-top: 10px
                padding-left: 0
                padding-right: 14px
                
          .row1
            order: 1
        
          .row2
            order: 2
          
          .row3
            order: 3
          
          .row4
            order: 4
          
          .row5
            order: 5
          
          .row6
            order: 6
            background-color: $color_white
          
          .row7
            order: 7
            background-color: #F0FBFE
          
          .row8
            order: 8
            background-color: $color_white
          
          .row9
            order: 9
            background-color: #F0FBFE
            
          .row10
            order: 10
            background-color: $color_white
            td
              &:first-child
                padding-left: 8px
  
  .busiest-rank
    .table
      width: 100%
      
    table
      th
        border-right: none
        &:first-child
          text-align: center
          padding-left: 8px
          padding-right: 0
          
        &:nth-child(2)
          padding-right: 0
          padding-left: 6px
          
        &:nth-child(3)
          display: none
          
        &:last-child
          padding-left: 0
          padding-right: 0
          text-align: center
          
      td
        &:first-child
          width: 5%
          padding-left: 2px
          padding-right: 0

        &:nth-child(2)
          padding-left: 6px

        &:nth-child(3)
          display: none
          
        &:last-child
          width: 10%
          padding-left: 0
          padding-right: 0
          
  .change-rank
    flex-direction: column
    &__table
      width: 100%
      .change-table
        th
          text-align: left
          font-size: 10px
          padding-left: 10px
          padding-right: 0
          border-right: none
          &:first-child
            padding-left: 14px

          &:nth-child(2)
            padding-left: 0 
            padding-right: 8px
            text-align: center   

        td
          &:first-child
            width: 70%
            padding-right: 0
            padding-left: 14px

          &:nth-child(2)
            width: auto
            padding-left: 12px
            padding-right: 12px

  .growth
    margin-bottom: 32px

  .ontime-best-rank
    .table
      width: 100%
      &__head
        top: 12px
        font-size: 12px
        line-height: 20px
    .small__table
      tbody  
        td
          padding-left: 10px
          &:first-child
            padding-left: 10px
            
          &:last-child
            padding-left: 0
            padding-right: 14px        
          

  .show_more
    display: flex
    justify-content: center
    align-items: center
    width: 100%
    height: 58px
    padding-top: 9px
    padding-bottom: 9px
    line-height: 18px
    box-sizing: border-box
    text-align: center
    &:after
      display: none


  .primary-title
    font-size: 20px
    padding-bottom: 22px
    line-height: 34px

  .cols__wrap
    flex-wrap wrap

  .bottom-col
    width: 100% 

  .block-title
    font-size 26px
    line-height: 34px

  .main-article--table
    .block-title
      font-size 26px
      line-height: 34px

  .related-list
    padding-top: 19px
    padding-left: 17px
    flex-direction column

  .related-item__link
    line-height 27px

  .related-list .col
    width: 100%

  .related-item
    padding-bottom: 22px

  .related
    margin-top: 17px

  .show-table
    text-align center
    font-size: 12px
    color: $main_color
    letter-spacing: 0
    line-height: 22px
    margin: 0 auto
    display: block
    span
      border-bottom: 1px dotted $main_color
      margin-left: 8px
    &:hover
      span
        border-bottom: none

  .mobile-table
    width: 100%
    margin-bottom: 16px

  .methodology-article
    padding-top: 110px
    padding-left: 0

  .methodology-article:before
    left: 50%
    top: 18px
    margin-left: -31px

  .mobile-only-show
    display: block

  .mobile-only-close
    font-style: normal
    -webkit-font-smoothing: antialiased
    -moz-osx-font-smoothing: grayscale
    text-align: center
    color: white
    text-decoration: none
    height: 41px
    text-transform: uppercase
    border-radius: 3px
    padding: 0 20px
    overflow: hidden
    font-size: 11px
    line-height: 41px
    font-weight bold
    position: fixed
    background-color $main_color
    top: 10px
    right: 25px
    .icon
      margin-right: 8px

  .colorful
    display block
    .breadcrumbs__item
      display inline-block

  .only_mob-crop
    width: 100px
    overflow: hidden
    white-space: nowrap
    display: inherit
    text-overflow: ellipsis
    vertical-align: top

  .main-article--table
    .breadcrumbs__item
      &:nth-child(3)
        overflow: hidden
        min-width: 90px
      
  .table
    display: flex
    flex-direction: row
    flex-wrap: wrap
    justify-content: flex-start
    align-items: center
    padding: 0 2px 2px
  
    &:before
      position: static
      margin-left: 16px
      margin-right: 16px
        
    &__head
      width: 60%
      position: static
      font-size: inherit
      min-height: 64px
      display: flex
      align-items: center
    
    .small__table
      width: 100%
      
  .ontime-rank
    .table
      &:before
        margin-top: auto
        
  .busiest-rank
    .table
      &:before
        margin-top: auto
    
  .mobile-table
    width: 100%
    margin-bottom: 16px

  .mobile-only-close
    display: none
    font-style: normal
    -webkit-font-smoothing: antialiased
    -moz-osx-font-smoothing: grayscale
    text-align: center
    color: white
    text-decoration: none
    height: 41px
    text-transform: uppercase
    border-radius: 3px
    padding: 0 20px
    overflow: hidden
    font-size: 11px
    line-height: 41px
    font-weight bold
    position: fixed
    background-color $main_color
    top: 10px
    right: 25px
    .icon
      margin-right: 8px
    
  .ranking-table
    .full-table
      display: none
    .mobile-only-show
      display: block

  .ranking-table.show
    padding-top: 20px
    margin-right: 15px
    margin-left: 15px
    margin-top: 45px
    .primary-text
      display: none
    .block-title
      font-size: 24px
      font-weight: 600
    .full-table
      display: table
      width: 100%
      overflow scroll
    .mobile-only-show
      display: none
    .mobile-only-close
      display: block

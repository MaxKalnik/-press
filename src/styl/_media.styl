@media (min-width: 1850px)
  .content
    width: 1360px

  .main-pic
    padding: 32px 60px 32px 60px
    background-position center
  
  .text-box
    padding: 64px 60px 64px 60px
    &--upper
      position: relative
      padding-left: 315px
  
  .upper-pic
    left: 60px
    
  .show_more
    margin-top: 44px
  
  .ontime-rank
    padding-right: 730px
  
  .busiest-rank
    padding-left: 607px
  
  .ontime-best-rank
    padding-right: 607px

@media (max-width: 1850px)
  .content
    width: 1220px

  .page__wrap
    background-image url(images/footer_bg.png) , url(images/svg/clouds_bg.svg) , none, linear-gradient(-180deg, #34c4f0 0, #1c90e2 100%)

  .main-pic
    padding-left: 60px
    height: 635px
  
  .related-head
    padding: 0 25px

  .related-list
    padding: 34px 25px 0

  .methodology-article
    padding-top: 23px
    padding-left: 141px
    &:before
      left: 20px


@media (min-width: 1019px) and (max-width: 1370px)
  .ontime-rank
    padding-right: 0
    .info-list
      &__item
        padding-right: 600px
        &:nth-child(n+3)
          padding-right: 0
          
    .table
      width: 565px
      td
        padding-left: 14px
        &:nth-child(2)
          padding-left: 12px
          font-size: 14px
          
        &:last-child
          padding-right: 14px
          
  .busiest-rank
    padding-left: 0
    .info-list
      &__item
        margin-left: 600px
        &:nth-child(n+4)
          margin-left: 0
          

  .change-rank
  &__table
    th
      padding-left: 22px
      &:nth-child(2)
        padding: 0
        text-align: center      
    td
      &:first-child
        width: 52%

      &:nth-child(2)
        width: auto
          

@media (max-width: 1370px)
  .content
    width: 980px
  
  .article-title
    font-size: 40px
    line-height: 68px
    font-weight: 600

  .article-text__item
    width: 650px
    font-size: 24px
    line-height: 30px
    font-weight: 300
    margin-bottom: 42px
    
  .article-author
    font-size: 12px
    line-height: 16px
    font-weight: 300

  .main-pic
    height: 568px
    background-repeat no-repeat
    background-position right top
    padding: 32px 40px 45px 45px

  .under-title
    margin-bottom: 32px

  .ontime-rank         
    .table
      width: 565px
          
  .change-rank
    &__table
      width: 425px
      
  .ontime-best-rank
    padding-right: 377px
    .table
      width: 330px
      &__head
        text-align: left
        top: 10px
      td
        padding-left: 20px
        &:first-child
          padding-left: 16px
        &:last-child
          padding-left: 16px
         
  .related-head
    padding: 0 20px

  .related-list
    padding-left: 32px

  .related-list .col
    width: 49%


  .text-box.last
    padding-top: 41px

  .bottom-col
    width: 48%
    padding-right: 8px

  .text-box.last
    display: block

@media (max-width: 1019px)
  .content
    width: auto
    max-width: 740px
    
  .article-title
    font-size: 32px
    line-height: 38px
    font-weight: 600

  .article-text__item
    width: 650px
    font-size: 24px
    line-height: 30px
    font-weight: 300
    margin-bottom: 12px
    
  .article-author
    font-size: 12px
    line-height: 16px
    font-weight: 300
    
  .main-pic
    height: 345px
    padding: 32px 25px 32px 25px
    background-image url("images/main_pic_768.png")
    background-image: -webkit-image-set(url(images/main_pic_768.png) 1x, url(images/main_pic_768@2x.png) 2x)
    background-position center

  .top-menu__link
    margin: 0 10px
    
  .text-box
    padding: 44px 25px 44px 25px
    &--upper
      display: flex
      flex-direction: row
      flex-wrap: wrap
      justify-content: space-between
      padding-left: 25px
      .upper-pic
        position: static
        width: 210px
      
      .under-title
        width: 445px
        margin-bottom: 32px

      .main-text
        width: 100%
    
  .calendar
    display: flex
    flex-direction: column
    padding-left: 0
    &__img
      position: static
      align-self: center
  
  .ontime-rank
    display: flex
    flex-direction: column
    padding-right: 0
    .table
      position: relative
      align-self: center
      width: 650px
      margin-bottom: 32px
    .info-list
      order: 2
      
  .busiest-rank
    display: flex
    flex-direction: column
    padding-left: 0
    .table
      position: relative
      align-self: center
      margin-bottom: 32px
    .info-list
      order: 2
      
  .change-rank
    &__table
      width: 330px   
    .change-table
      th
        padding: 0 10px
        padding-left: 24px
        &:nth-child(2)
          padding-left: 18px
          text-align: left
              
      td
        &:first-child
          width: 60%
          padding-right: 0
        
        &:nth-child(2)
          width: auto
          
  .ontime-best-rank
    display: flex
    flex-direction: row
    justify-content: space-between
    flex-wrap: wrap
    padding-right: 0
    .table
      top: 0
      position: relative
      width: 330px
      margin-bottom: 32px
    .info-list
      order: 2
    .show_more
      order: 3
      margin: auto
      margin-top: 32px
      

  .block-title
    margin-bottom: 32px

  .related-head
    padding: 0 20px

  .related-list
    padding-left: 21px

  .related-list .col
    width: 46%

  .methodology-article
    padding-left: 97px
    padding-right: 0
    padding-top: 23px
    &:before
      left: 8px
      top: 27px

  .methodology-title
    text-align: center

  .bottom-col
    width: 49%
    padding-right: 0px

  .text-box.last
    padding-top: 25px



@media (min-width: 759px)
  .row6
    background-color: #F0FBFE

  .row7
    background-color: $color_white

  .row8
    background-color: #F0FBFE

  .row9
    background-color: $color_white

  .row10
    background-color: #F0FBFE
  
  .mobile-only-close
    display: none
       
  .mobile-only-show
    display: none
    
 .ranking-table.show
  padding-top: 20px
  margin-right: 15px
  margin-left: 15px
  margin-top: 45px
  .primary-text
    display: none
  .block-title
    font-size: 26px
